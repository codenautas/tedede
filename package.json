{
  "name": "tedede",
  "description": "TDD",
  "version": "0.0.1",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/tedede",
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "dependencies": {
    "fs-extra": "0.26.3",
    "fs-promise": "0.3.1",
    "js-yaml": "3.4.6",
    "lodash": "3.10.1",

    "ajax-best-promise": "0.0.7",    
    "best-globals": "0.1.3",
    "best-promise": "0.2.4",
    "js-to-html": "0.6.4"
  },
  "devDependencies": {
    "body-parser": "1.14.2",
    "casperjs": "1.1.0-beta3",
    "cookie-parser": "1.4.0",
    "es6-promise": "3.0.2",
    "expect.js": "0.3.1",
    "express": "4.13.3",
    "express-session": "1.12.1",
    "express-useragent": "0.2.4",
    "istanbul": "0.4.1",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-expect": "1.1.1",
    "karma-firefox-launcher": "0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "0.2.1",
    "karma-phantomjs-launcher": "0.2.2",
    "karma-safari-launcher": "0.1.1",
    "jade": "1.11.0",
    "mocha": "2.3.4",
    "read-yaml-promise": "1.0.2",
    "sinon": "1.17.2",
    
    "extension-serve-static": "0.1.6"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "scripts": {
    "infinito": "node server/karma-server.js --karma karma.conf.js",
    "test": "node server/karma-server.js --karma karma.conf.js --single-run",
    "test-cov": "node server/karma-server.js --karma karma.conf.js --single-run",
    "ptest": "node server/pdemo-server.js",
    "ptest-h": "node server/pdemo-server.js --p--hold",
    "ctest": "node server/pdemo-server.js --use-casper",
    "ctest-h": "node server/pdemo-server.js --use-casper --p--hold",
    "start": "node server/server.js"
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true
  },
  "eslintConfig":{
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    }
  },
  "qa-control": {
    "package-version": "0.1.4",
    "coverage": 90,
    "run-in": "server",
    "stability": "designing",
    "type": "cmd-tool"
  }
}
